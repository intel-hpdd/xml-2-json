{
  "name": "intel-xml-2-json",
  "version": "1.0.0",
  "description": "parses simple xml to JSON output",
  "main": "dist/source/xml-2-json.js",
  "scripts": {
    "test:dev": "npm run prepublish && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "eslint": "eslint ./",
    "jscs": "jscs .",
    "prepublish": "./bin/build.sh",
    "cover:dev": "npm run cover -- --reporter=html --reporter=lcov",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t"
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "jscs",
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:IML/xml-parser.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.2",
    "babel-eslint": "5.0.0",
    "babel-plugin-__coverage__": "0.1111.0",
    "babel-plugin-transform-flow-strip-types": "6.7.0",
    "babel-plugin-transform-strict-mode": "6.6.5",
    "babel-preset-es2015": "6.6.0",
    "eslint": "2.0.0",
    "eslint-plugin-flow-vars": "0.2.1",
    "intel-jasmine-junit-reporter": "1.0.0",
    "intel-jasmine-n-matchers": "2.1.1",
    "jasmine": "2.4.1",
    "jscs": "2.11.0",
    "nyc": "6.1.1",
    "pre-commit": "1.1.2"
  },
  "dependencies": {
    "libxmljs": "0.17.1"
  },
  "nyc": {
    "include": [
      "/"
    ],
    "exclude": [
      "test"
    ]
  }
}
