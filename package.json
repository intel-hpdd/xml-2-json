{
  "name": "@mfl/xml-2-json",
  "version": "1.0.1",
  "description": "parses simple xml to JSON output",
  "main": "dist/source/xml-2-json.js",
  "scripts": {
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t",
    "eslint": "eslint ./",
    "flow": "flow",
    "postversion": "./bin/build.sh"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "access": "public"
  },
  "pre-commit": [
    "flow",
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:MFL/xml-parser.git"
  },
  "author": "MFL Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-strict-mode": "^6.22.0",
    "babel-preset-node6": "11.0.0",
    "eslint": "^3.18.0",
    "eslint-config-prettier": "^1.5.0",
    "eslint-plugin-flowtype": "^2.30.4",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-prettier": "^2.0.1",
    "flow-bin": "^0.42.0",
    "@mfl/jasmine-junit-reporter": "1.0.0",
    "@mfl/jasmine-n-matchers": "2.1.1",
    "jasmine": "^2.5.3",
    "nyc": "^10.1.2",
    "pre-commit": "^1.2.2",
    "prettier": "^0.22.0"
  },
  "dependencies": {
    "node-xml": "^1.0.2"
  }
}
