{
  "name": "intel-xml-2-json",
  "version": "1.0.1",
  "description": "parses simple xml to JSON output",
  "main": "dist/source/xml-2-json.js",
  "scripts": {
    "test:dev": "npm run postversion && npm t",
    "test": "NODE_ENV=test ./bin/test.js",
    "cover:dev": "npm run cover -- --reporter=html",
    "cover": "NODE_ENV=test ./bin/build.sh cover && nyc --reporter=text npm t",
    "eslint": "eslint ./",
    "flow": "flow",
    "postversion": "./bin/build.sh"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "publishConfig": {
    "registry": "https://ubit-artifactory-or.intel.com/artifactory/api/npm/iml-npm-local"
  },
  "pre-commit": [
    "flow",
    "eslint",
    "test:dev"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.intel.com:IML/xml-parser.git"
  },
  "author": "IML Team",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "7.0.0",
    "babel-plugin-__coverage__": "11.0.0",
    "babel-plugin-transform-flow-strip-types": "6.14.0",
    "babel-plugin-transform-strict-mode": "6.11.3",
    "babel-preset-node6": "11.0.0",
    "eslint": "3.6.1",
    "eslint-plugin-flowtype": "2.19.0",
    "eslint-plugin-import": "1.16.0",
    "flow-bin": "0.32.0",
    "intel-jasmine-junit-reporter": "1.0.0",
    "intel-jasmine-n-matchers": "2.1.1",
    "jasmine": "2.5.2",
    "nyc": "8.3.0",
    "pre-commit": "1.1.3"
  },
  "dependencies": {
    "libxmljs": "0.18.0"
  }
}
